{
  "expo": {
    "name": "Uncanon",
    "slug": "uncanon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./graphics/appicon.png",
    "splash": {
      "image": "./graphics/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./graphics/appicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "androidStatusBar": {
      "translucent": false
    },
    "web": {
      "favicon": "./graphics/favicon.png"
    }
  }
}
